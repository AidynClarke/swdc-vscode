{
    "name": "swdc-vscode",
    "displayName": "Code Time",
    "version": "0.16.10",
    "publisher": "softwaredotcom",
    "description": "Code Time is an open source plugin that provides programming metrics right in Visual Studio Code.",
    "author": {
        "name": "Software.com"
    },
    "license": "SEE LICENSE IN LICENSE",
    "icon": "images/app-icon-blue.png",
    "galleryBanner": {
        "color": "#384356",
        "theme": "dark"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/swdotcom/swdc-vscode"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "time tracking",
        "productivity",
        "timer",
        "git",
        "spotify"
    ],
    "activationEvents": [
        "*"
    ],
    "engines": {
        "vscode": "^1.19.0"
    },
    "main": "./out/extension",
    "scripts": {
        "prebuildcodetime": "node deployer code-time && tsc -watch -p ./",
        "prebuildmusictime": "node deployer music-time && tsc -watch -p ./",
        "musictime-package": "node deployer music-time && tsc -p ./",
        "codetime-package": "node deployer ode-time && tsc -p ./",
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./"
    },
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "Code Time",
            "properties": {
                "showMusicMetrics": {
                    "type": "boolean",
                    "default": false,
                    "description": "Music metrics"
                },
                "showGitMetrics": {
                    "type": "boolean",
                    "default": false,
                    "description": "Git metrics"
                },
                "showWeeklyRanking": {
                    "type": "boolean",
                    "default": false,
                    "description": "Weekly Ranking"
                }
            }
        },
        "commands": [],
        "menus": {
            "view/title": [
                {
                    "when": "view == music-time-playlists",
                    "group": "navigation"
                },
                {
                    "when": "view == music-time-settings",
                    "group": "navigation"
                }
            ]
        }
    },
    "devDependencies": {
        "@types/mocha": "5.2.6",
        "@types/node": "12.0.0",
        "typescript": "^3.4.5",
        "@types/vscode": "^1.32.0"
    },
    "dependencies": {
        "cody-music": "2.4.0",
        "vsls": "0.3.1291",
        "axios": "^0.19.0"
    }
}