{
    "name": "swdc-vscode",
    "displayName": "Code Time",
    "version": "1.4.0",
    "publisher": "softwaredotcom",
    "description": "Code Time is an open source plugin that provides programming metrics right in Visual Studio Code.",
    "author": {
        "name": "Software.com"
    },
    "license": "SEE LICENSE IN LICENSE",
    "icon": "images/app-icon-blue.png",
    "galleryBanner": {
        "color": "#384356",
        "theme": "dark"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/swdotcom/swdc-vscode"
    },
    "remote.extensionKind": {
        "softwaredotcom.swdc-vscode": "ui"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "time tracking",
        "productivity",
        "timer",
        "git",
        "spotify"
    ],
    "activationEvents": [
        "*"
    ],
    "engines": {
        "vscode": "^1.35.0"
    },
    "main": "./out/extension",
    "scripts": {
        "start": "node initializer && tsc -watch -p ./",
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./"
    },
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "Code Time",
            "properties": {
                "showGitMetrics": {
                    "type": "boolean",
                    "default": false,
                    "description": "Git metrics"
                },
                "toggleFileEventLogging": {
                    "type": "boolean",
                    "default": false,
                    "description": "Enable file event logging"
                }
            }
        },
        "commands": [],
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "code-time",
                    "title": "Code Time",
                    "icon": "resources/dark/cody.svg"
                }
            ]
        },
        "views": {
            "code-time": [
                {
                    "id": "kpm-metrics-tree",
                    "name": "Keystroke Metrics"
                },
                {
                    "id": "commit-tree",
                    "name": "Git Status"
                },
                {
                    "id": "file-change-tree",
                    "name": "File Leaderboard"
                }
            ]
        }
    },
    "devDependencies": {
        "@types/mocha": "5.2.6",
        "@types/node": "12.0.4",
        "typescript": "^3.5.1",
        "@types/vscode": "^1.35.0"
    },
    "dependencies": {
        "@slack/web-api": "^5.0.1",
        "aws-sdk": "^2.470.0",
        "axios": "^0.19.0",
        "clipboardy": "^2.1.0",
        "cody-music": "2.6.70",
        "moment-timezone": "^0.5.25",
        "open": "^6.4.0",
        "vsls": "0.3.1291"
    }
}
