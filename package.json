{
    "name": "swdc-vscode",
    "displayName": "Code Time",
    "version": "1.1.13",
    "publisher": "softwaredotcom",
    "description": "Code Time is an open source plugin that provides programming metrics right in Visual Studio Code.",
    "author": {
        "name": "Software.com"
    },
    "license": "SEE LICENSE IN LICENSE",
    "icon": "images/app-icon-blue.png",
    "galleryBanner": {
        "color": "#384356",
        "theme": "dark"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/swdotcom/swdc-vscode"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "time tracking",
        "productivity",
        "timer",
        "git",
        "spotify"
    ],
    "activationEvents": [
        "*"
    ],
    "engines": {
        "vscode": "^1.35.0"
    },
    "main": "./out/extension",
    "scripts": {
        "prebuildcodetime": "node deployer code-time && tsc -watch -p ./",
        "prebuildmusictime": "node deployer music-time && tsc -watch -p ./",
        "musictime-package": "node deployer music-time && vsce package",
        "codetime-package": "node deployer code-time && vsce package",
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./"
    },
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "Code Time",
            "properties": {
                "showMusicMetrics": {
                    "type": "boolean",
                    "default": false,
                    "description": "Music metrics"
                },
                "showGitMetrics": {
                    "type": "boolean",
                    "default": false,
                    "description": "Git metrics"
                },
                "showWeeklyRanking": {
                    "type": "boolean",
                    "default": false,
                    "description": "Weekly Ranking"
                }
            }
        },
        "commands": []
    },
    "devDependencies": {
        "@types/mocha": "5.2.6",
        "@types/node": "12.0.4",
        "typescript": "^3.5.1",
        "@types/vscode": "^1.35.0"
    },
    "dependencies": {
        "aws-sdk": "^2.470.0",
        "axios": "^0.19.0",
        "clipboardy": "^2.1.0",
        "cody-music": "2.6.1",
        "moment-timezone": "^0.5.25",
        "vsls": "0.3.1291"
    }
}