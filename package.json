{
    "name": "swdc-vscode",
    "displayName": "Software.com",
    "version": "0.5.14",
    "publisher": "softwaredotcom",
    "description": "Software.com for VS Code - It's Code Time",
    "author": {
        "name": "Software.com"
    },
    "license": "SEE https://software.com/eula",
    "icon": "images/icon.png",
    "galleryBanner": {
        "color": "#03bdf2",
        "theme": "dark"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/swdotcom/swdc-vscode"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "engines": {
        "vscode": "^1.19.0"
    },
    "main": "./out/extension",
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "contributes": {
        "commands": [
            {
                "command": "extension.softwareKpmDashboard",
                "title": "Go to software.com",
                "category": "Software.com"
            },
            {
                "command": "extension.pauseSoftwareMetrics",
                "title": "Pause metrics",
                "category": "Software.com"
            },
            {
                "command": "extension.enableSoftwareMetrics",
                "title": "Enable metrics",
                "category": "Software.com"
            }
        ],
        "menus": {
            "view/item/context": [
                {
                    "command": "extension.softwareKpmDashboard",
                    "group": "Software.com"
                },
                {
                    "command": "extension.pauseSoftwareMetrics",
                    "group": "Software.com"
                },
                {
                    "command": "extension.enableSoftwareMetrics",
                    "group": "Software.com"
                }
            ]
        }
    },
    "devDependencies": {
        "@types/axios": "^0.14.0",
        "@types/node": "^9.4.0",
        "typescript": "^2.6.2"
    },
    "dependencies": {
        "axios": "^0.17.1",
        "itunes-node-applescript": "0.3.5",
        "open": "^0.0.5",
        "spotify-node-applescript": "1.1.1",
        "vscode": "^1.1.10"
    }
}
